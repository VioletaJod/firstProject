package lt.varle;

import java.text.DecimalFormat;

//3.1 Sugeneruokite 300 atsitiktinių skaičių nuo 0 iki 300, atspausdinkite juos atskirtus tarpais ir suskaičiuokite
////        kiek tarp jų yra didesnių už 150.  Skaičiai didesni nei 275 turi būti atspausdinti skliausteliuose” [ ] “.
//
public class Main {
    public static void main(String[] args) {
       double sk;
       int max = 300;
       int counter = 0;
       for (int i = 0; i < 300; i++){
           sk = Math.floor(Math.random()*max);
           if (sk <= 275 ){
               System.out.print(sk + " " + "");
           }
           else if(sk >275){
               System.out.print(" \n [ "+ sk +"] ");
           }
           if (sk >150){
               counter++;
           }

        }
        System.out.print(" \n\n Reikšmių didesnių nei 150 buvo:" + counter);
    }
}
